<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>plusSeven</title>
  </head>
  <body>
    <header class="site-head">
      <!-- topbar -->
      {{include '../../components/topbar/topbar.art'}}
      <!-- main-nav -->
      {{include '../../components/main-nav/main-nav.art'}}
      <!-- header-con -->
      {{include '../../components/header-con/header-con.art'}}
    </header>
    
    <!-- carousel -->
    <section class="banner" id="banner">
      <!-- 轮播图接口有使用期限，这里使用本地资源 -->
      {{include './components/slider/slider.art'}}
      <div class="center-wrap">
        {{include './components/slider/btn.art'}}
        <div class="v-menu-box" id="v-menu-box">
          {{include './components/menu/v-menu.art'}}
          {{include './components/menu/menus.art'}}
        </div>
      </div>
    </section>

    <!-- fav-ad -->
    <section class="layer fav-ad">
      <div class="center-wrap">
        <% include ('../../components/loading/loading.art', {text:'正在拼命为您加载...'}) %>
      </div>
    </section>

    <!-- xxsw -->
    <section class="layer xxsw">
      <div class="center-wrap">
        {{include './components/xxsw/xxsw.art'}}
      </div>
    </section>

    <!-- jjzyx -->
    <section class="layer layer-common-style jjzyx">
      <div class="center-wrap">
        <!-- 机酒自由行接口有使用期限，这里使用本地资源 -->
        {{include './components/jjzyx/jjzyx.art'}}
      </div>
    </section>

    <!-- ddwl -->
    <section class="layer ddwl">
      <div class="center-wrap">
        {{include './components/ddwl/ddwl.art'}}
      </div>
    </section>

    <!-- tsddty -->
    <section class="layer tsddty">
      <div class="center-wrap">
        {{include './components/tsddty/tsddty.art'}}
      </div>
    </section>

    <!-- zsj -->
    <section class="layer layer-common-style another-style zsj">
      <div class="center-wrap">
        {{include './components/zsj/zsj.art'}}
      </div>
    </section>

    <!-- gt -->
    <section class="layer layer-common-style another-style gt">
      <div class="center-wrap">
        {{include './components/gt/gt.art'}}
      </div>
    </section>

    <footer>
      <!-- footer-nav -->
      {{include '../../components/footer-nav/footer-nav.art'}}
      <!-- bottom-bar -->
      {{include '../../components/bottom-bar/bottom-bar.art'}}
      <!-- backtotop -->
      {{include '../../components/backtotop/backtotop.art'}}
    </footer>
  </body>
</html>
